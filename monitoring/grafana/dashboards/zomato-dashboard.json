        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 7,
        "title": "OpenKruise WorkloadSpread Status",
        "type": "stat",
        "targets": [
          {
            "expr": "kruise_workloadspread_status_replicas",
            "legendFormat": "{{namespace}}/{{name}}"
