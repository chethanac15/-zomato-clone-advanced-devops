{
  "name": "zomato-clone-advanced",
  "version": "1.0.0",
  "description": "Advanced Zomato clone with OpenKruise integration, comprehensive monitoring, and enterprise-grade DevOps features",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "k6 run testing/k6/load-test.js",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "security:audit": "npm audit && snyk test",
    "security:fix": "npm audit fix",
    "docker:build": "docker build -t zomato-clone:latest .",
    "docker:run": "docker run -p 3000:3000 zomato-clone:latest",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "terraform:init": "cd terraform && terraform init",
    "terraform:plan": "cd terraform && terraform plan",
    "terraform:apply": "cd terraform && terraform apply",
    "terraform:destroy": "cd terraform && terraform destroy",
    "jenkins:validate": "curl -X POST http://localhost:8080/job/zomato-pipeline/build --user admin:$(cat jenkins/admin-api-token.txt)",
    "argocd:sync": "argocd app sync zomato-app",
    "monitoring:setup": "kubectl apply -f monitoring/",
    "security:setup": "kubectl apply -f security/",
    "openkruise:setup": "kubectl apply -f k8s/openkruise/",
    "chaos:test": "kubectl apply -f testing/chaos/",
    "docs:generate": "jsdoc src/ -d docs/",
    "docs:serve": "http-server docs/ -p 8080",
    "performance:benchmark": "artillery run testing/artillery/config.yml",
    "load:test": "k6 run testing/k6/load-test.js --out json=results/k6-results.json",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "metrics:check": "curl -f http://localhost:3000/metrics || exit 1",
    "precommit": "npm run lint && npm run test && npm run security:audit",
    "postinstall": "echo 'Installing additional tools...' && npm run setup:tools",
    "setup:tools": "npm install -g k6 artillery newman",
    "clean": "rm -rf node_modules package-lock.json coverage dist docs results",
    "build:prod": "npm run clean && npm install --production && npm run test && npm run docker:build",
    "deploy:staging": "npm run k8s:deploy && npm run monitoring:setup && npm run health:check",
    "deploy:production": "npm run terraform:apply && npm run k8s:deploy && npm run monitoring:setup && npm run security:setup"
  },
  "keywords": [
    "zomato",
    "clone",
    "devops",
    "kubernetes",
    "openkruise",
    "microservices",
    "monitoring",
    "ci-cd",
    "terraform",
    "argocd",
    "prometheus",
    "grafana",
    "jenkins",
    "docker",
    "aws",
    "eks",
    "cloud-native",
    "enterprise"
  ],
